{
  "name": "generator-gonerator",
  "version": "0.0.1",
  "description": "Yet another microservice boilerplate for GoLang",
  "homepage": "",
  "author": {
    "name": "Onur Kapçık",
    "email": "onurkapcik@gmail.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "go",
    "microservice",
    "boilerplate",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.6.0",
    "eslint-config-xo": "0.27.2",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.9",
    "jest": "26.1.0",
    "lint-staged": "9.4.3",
    "prettier": "1.19.1",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "1.7.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.10.0"
  },
  "dependencies": {
    "chalk": "2.1.0",
    "mkdirp": "1.0.4",
    "yeoman-generator": "5.6.1",
    "yosay": "2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "kapozade/generator-gonerator",
  "license": "MIT"
}
